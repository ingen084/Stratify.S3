{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Stratify.S3": "Information"
    }
  },
  "AppSettings": {
    "ChunkSize": 65536,
    "MaxFileSize": 107374182400,
    "HealthCheckFile": ".s3proxy_health",
    "MetadataDir": ".s3proxy_metadata",
    "AutoRecoveryEnabled": true,
    "RecoveryCheckInterval": 600,
    "RecoveryBatchSize": 20,
    "RecoveryTimeout": 600,
    "PreferPrimary": true
  },
  "Authentication": {
    "Enabled": true,
    "Mode": "Both",
    "TokenExpirationMinutes": 30,
    "ApiKeys": [
      {
        "Name": "production-admin",
        "Key": "change-this-in-production",
        "AllowedOperations": ["*"],
        "AllowedBuckets": ["*"],
        "Enabled": true
      }
    ],
    "AwsCredentials": [
      {
        "AccessKeyId": "CHANGE_THIS_ACCESS_KEY",
        "SecretAccessKey": "CHANGE_THIS_SECRET_KEY",
        "Name": "production-aws",
        "AllowedOperations": ["*"],
        "AllowedBuckets": ["*"],
        "Enabled": true
      }
    ]
  }
}